			"        h[u] = cnt++;",
			"    }",
			"    int dis[N], vis[N], n;",
			"",
			"    int spfa(int s, int d[]){",
			"        queue < int > q;",
			"        memset(vis, 0, sizeof(vis));",
			"        for(int i = 0; i <= n; i++) d[i] = inf; //最短路",
			"        d[s] = 0;",
