			"        LL w;",
			"    }e[N];",
			"",
			"    bool cmp(struct sa x,struct sa y){",
			"        return x.w < y.w;",
			"    }",
			"",
			"    void init(){",
			"        // n点，m条边",
			"        cin >> n >> m;",
			"        for(int i = 1; i <= m; i++) cin >> e[i].u >> e[i].v >> e[i].w;",
			"        sort(e + 1, e + 1 + m, cmp);",
			"        for(int i = 1; i <= n; i++) fa[i] = i;",
			"    }",
			"",
			"    int find(int x){",
			"        return fa[x] == x ? x : fa[x] = find(fa[x]);",
			"    }",
			"",
			"",
			"    void solve(){",
			"        for(int i = 1; i <= m; i++){",
			"            if(cnt == n - 1) break;",
